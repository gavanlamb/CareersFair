<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <WebPublishMethod>MSDeploy</WebPublishMethod>
    <LastUsedBuildConfiguration>Release</LastUsedBuildConfiguration>
    <LastUsedPlatform>Any CPU</LastUsedPlatform>
    <SiteUrlToLaunchAfterPublish>htt://localhost/CareersFair</SiteUrlToLaunchAfterPublish>
    <LaunchSiteAfterPublish>True</LaunchSiteAfterPublish>
    <ExcludeApp_Data>True</ExcludeApp_Data>
    <MSDeployServiceURL>localhost</MSDeployServiceURL>
    <DeployIisAppPath>Default Web Site/CareersFair</DeployIisAppPath>
    <RemoteSitePhysicalPath />
    <SkipExtraFilesOnServer>True</SkipExtraFilesOnServer>
    <MSDeployPublishMethod>InProc</MSDeployPublishMethod>
    <EnableMSDeployBackup>False</EnableMSDeployBackup>
    <UserName />
    <_SavePWD>False</_SavePWD>
    <PublishDatabaseSettings>
      <Objects xmlns="">
        <ObjectGroup Name="FormContext" Order="1" Enabled="False">
          <Destination Path="Data Source=.\SQLExpress;Initial Catalog=asp.net-CareersFair;Integrated Security=True;Pooling=False" />
          <Object Type="DbCodeFirst">
            <Source Path="DBContext" DbContext="CareersFair.Models.ApplicationDbContext, CareersFair" Origin="Configuration" />
          </Object>
        </ObjectGroup>
      </Objects>
    </PublishDatabaseSettings>
  </PropertyGroup>
  <ItemGroup>
    <MSDeployParameterValue Include="$(DeployParameterPrefix)FormContext-Web.config Connection String">
      <ParameterValue>Data Source=.\SQLExpress;Initial Catalog=asp.net-CareersFair;Integrated Security=True;Pooling=False</ParameterValue>
    </MSDeployParameterValue>
  </ItemGroup>
</Project>